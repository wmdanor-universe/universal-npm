{
  "name": "universal-npm",
  "version": "1.1.0",
  "description": "Universal Node.js Package Manager",
  "license": "Apache-2.0",
  "author": "wmdanor",
  "repository": "https://github.com/wmdanor-universe/universal-npm",
  "keywords": [
    "unpm",
    "universal",
    "package manager",
    "npm",
    "yarn",
    "pnpm",
    "proxy",
    "aggregating"
  ],
  "scripts": {
    "build": "npm run swc && npm run pregenerate-files",
    "dev": "concurrently \"npm:typecheck:watch\" \"npm:swc:watch\" \"npm:pregenerate-files:watch\"",
    "test": "jest",
    "swc": "swc src -d dist --delete-dir-on-start",
    "swc:watch": "swc src -d dist --delete-dir-on-start --watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch --incremental",
    "pregenerate-files": "ts-node scripts/pregenerate-files.ts",
    "pregenerate-files:watch": "nodemon --exec \"npm run pregenerate-files\" --watch dist",
    "lint": "eslint src/**/* scripts/**/*",
    "release": "ts-node scripts/release.ts",
    "create-command": "ts-node scripts/create-command.ts"
  },
  "files": [
    "dist",
    "generated"
  ],
  "bin": {
    "unpm": "dist/bin/unpm.js"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.0",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.0",
    "ajv": "^8.12.0",
    "preferred-pm": "^3.0.3",
    "yargs": "^17.7.0"
  }
}
